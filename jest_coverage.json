{
  "automock": false,
  "moduleFileExtensions": [
    "js"
  ],
  "testRegex": "lib/es6_global/test/.+test\\.js$",
  "transformIgnorePatterns": [
    "<rootDir>/node_modules/(?!(.*bs-)|reason-react|immutable-re|wonder-commonlib|wonder.js|wonder-log|wonder-most-animation-frame)"
  ],
  "setupTestFrameworkScriptFile": "<rootDir>/test/helper/jest/specHelper.js",
  "notify": false,
  "verbose": true,
  "collectCoverage": true,
  "coverageDirectory": "<rootDir>/coverage/",
  "coveragePathIgnorePatterns": [
    "<rootDir>/lib/es6_global/src/config/",
    "<rootDir>/lib/es6_global/src/extension/config/data/extension_component/",
    "<rootDir>/lib/es6_global/src/core/config/",
    "<rootDir>/lib/es6_global/src/core/.+/.+Type\\.js",
    "<rootDir>/lib/es6_global/src/core/utils/",
    "<rootDir>/lib/es6_global/src/core/external/",
    "<rootDir>/lib/es6_global/src/core/ui/store/",
    "<rootDir>/lib/es6_global/src/core/redux/",
    "<rootDir>/lib/es6_global/src/service/state/engine/AssetEngineService\\.js",
    "<rootDir>/lib/es6_global/src/service/state/engine/LoopEngineService\\.js",
    "<rootDir>/lib/es6_global/src/service/state/engine/BasicSourceTextureEngineService\\.js",
    "<rootDir>/lib/es6_global/src/service/atom/SparseMapService\\.js",
    "<rootDir>/lib/es6_global/src/service/atom/ArrayService\\.js",
    "<rootDir>/lib/es6_global/src/service/atom/FloatService\\.js",
    "<rootDir>/lib/es6_global/src/core/Index\\.js",
    "<rootDir>/lib/es6_global/src/core/ui/App\\.js",
    "<rootDir>/lib/es6_global/src/core/composable_component/mainEditor/ui/MainEditor\\.js",
    "<rootDir>/lib/es6_global/src/core/ui/eventHandler/EmptyEventHandler\\.js"
  ],
  "collectCoverageFrom": [
    "lib/es6_global/src/**/*.{js}",
    "!**/node_modules/**",
    "!**/vendor/**"
  ],
  "coverageThreshold": {
    "global": {
      "lines": 95,
      "statements": 95,
      "branches": 85,
      "functions": 95
    }
  }
}